{
  "expo": {
   
    "name": "reactnav",
    "slug": "reactnav",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "androidStatusBar":{
      "backgroundColor":"#9e9999",
      "translucent": false
    },

    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/images/img_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
